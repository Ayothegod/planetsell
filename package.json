    "clsx": "^2.1.0",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.30.2",
    "embla-carousel-react": "^8.0.0",
    "esbuild-register": "^3.5.0",
    "lucia": "^3.1.1",
